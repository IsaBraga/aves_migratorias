#adicionar variaveis climaticas anuais no modelo (baixados do ecoclimate)
library(raster)

files<-dir(pattern=".txt")

for(i in 1:length(files)){
modelo<-read.table(files[i],h=T)

modelo_rst<-stack(rasterFromXYZ(data.frame(modelo[,c(2:3)],modelo[,c(4:22)]))) 
writeRaster(modelo_rst, gsub(".txt", ".grd", files0k[i]))

}
